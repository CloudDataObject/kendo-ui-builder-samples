{
    "title": "<%- title %>",
    "editMode" : "<%- editMode %>",
    "options": {
        "pageable": {
            <% if (pageSize) { %>
            "pageSize": <%= pageSize %>,
            <% } %>
            "refresh": true
        },
        "filterable": <%= filterable %>,
        "groupable": <%= groupable %>,
        "resizable": <%= resizable %>,
        "reorderable": <%= reorderable %>,
        "sortable": <%= sortable %>,
        "selectable": true,
        "columns": <%- columns %>,
        "dataSource": <%- dataSource %>
    }
}