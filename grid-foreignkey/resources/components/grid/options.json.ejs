{
    widget: null,
    options: {
        pageable: {
            refresh: true
        },
        <% if (typeof toolbar !== "undefined") { %>
            toolbar: <%- toolbar %>,
        <% } %>
        editable: '<%= editable.toLowerCase(); %>',
        <% if (selectable !== "None") { %>
        <%  var selectionType = { Single: 'row', Multiple: 'multiple, row' }; %>
        selectable: "<%= selectionType[selectable] %>",
        <% } %>
        filterable: <%= filterable %>,
        groupable: <%= groupable %>,
        resizable: <%= resizable %>,
        reorderable: <%= reorderable %>,
        sortable: <%= sortable %>,
        columns: <%- columns %>,
        dataSource: $scope._$ds['<%- dataSourceName %>'] <% if (rowTemplateId !== "") { %>,
        rowTemplate: function(dataItem) {
            return kendo.template(angular.element("#<%- rowTemplateId %>").html())(dataItem);
        }<% } %><% if (rowTemplateId === "" && rowTemplateFunction !== "" && view.extension) { %>,
        rowTemplate: function(dataItem) { 
            return $injector.get('<%- view.extension %>')['<%- rowTemplateFunction %>'](dataItem);
        } <% } %>
    },
    events: {
        onRowSelect: function(e) {
        <% if (view.extension && events.onRowSelect) { %>
            $scope.factory['<%- events.onRowSelect %>'](e);
        <% } %>
        },
        onDataBound: function(e) {
        <% if (view.extension && events.onDataBound) { %>
            $scope.factory['<%- events.onDataBound %>'](e);
        <% } %>
        }
    }
}