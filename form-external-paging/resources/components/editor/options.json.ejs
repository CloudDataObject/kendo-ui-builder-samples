{
    widget: null,
    options: {
        <% if (typeof resizable !== "undefined") { %>
            resizable: <%- resizable %>,
        <% } %>
        <% if (typeof tools !== "undefined") { %>
            tools: <%- JSON.stringify(tools) %>,
        <% } %>
        encoded: <%- encoded %>
    },
    events: {
        onChange: function(e) {
            <% if (events.onChange) { %>
                 $scope.factory['<%- events.onChange %>'](e);
            <% } %>
        }
    },
    validation: {
        required: <%- validation.required %>
    }
}