{
    widget: null,
    options: {<% if (dataSourceName) { %>
       dataSource: $scope._$ds['<%- dataSourceName %>'], <% } %> 
       dataTextField: "<%- textField %>",
       dataValueField: "<%- valueField %>",
       valuePrimitive: <%- valuePrimitive %>,
       filter: "<%- filter.toLowerCase() %>"
    },
    events: {
        onChange: function(e) {
            <% if (events.onChange) { %>
                 $scope.factory['<%- events.onChange %>'](e);
            <% } %>
        },
        onSelect: function(e) {
            <% if (events.onSelect) { %>
                 $scope.factory['<%- events.onSelect %>'](e);
            <% } %>
        },
        onFilter: function(e) {
            <% if (events.onFilter) { %>
                 $scope.factory['<%- events.onFilter %>'](e);
            <% } %>
        }
    },
    validation: {
        required: <%- validation.required %>
    }
}