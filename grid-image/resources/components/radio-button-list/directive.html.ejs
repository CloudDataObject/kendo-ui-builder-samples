<radio-button-list 
    data-id="<%- id %>"
    data-events="<%- id %>.events"
    data-default-value="<%- defaultValue %>"
    <% if (model) { %>
    data-model="_$viewModels.<%- model %>",
    <% } else { %>
    data-model="_$viewModels.<%- id %>",
    <% } %>
    data-true-text="<%- trueText %>"
    data-false-text="<%- falseText %>">
</radio-button-list>