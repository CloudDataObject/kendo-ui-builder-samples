{
    widget: null,
    options: {
    <% if (typeof mask !== "undefined") { %> 
        mask: "<%- mask %>",
    <% } %>
    <% if (typeof promptChar !== "undefined") { %> 
        promptChar: "<%- promptChar %>",
    <% } %>
        value: "<%- defaultValue %>"
    },
    events: {
        onChange: function(e) {
            <% if (view.extension && events.onChange) { %>
            $scope.factory['<%- events.onChange %>'](e);
            <% } %>
        }
    },
    validation: {
        required: <%- validation.required %>
    }
}