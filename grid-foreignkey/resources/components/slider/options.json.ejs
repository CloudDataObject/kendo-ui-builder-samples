{
    widget: null,
    options: {
    <% if (typeof defaultValue !== "undefined") { %>
         value: <%- defaultValue %> , 
    <% } %>
    <% if (typeof min !== "undefined") { %>
         min: <%- min %> , 
    <% } %>
    <% if (typeof max !== "undefined") { %>
         max: <%- max %>, 
    <% } %>
    <% if (typeof step !== "undefined") { %>
         smallStep: <%- step %>, 
    <% } %>
    <% if (typeof showButtons !== "undefined") { %>
         showButtons: <%= showButtons %>, 
    <% } %>
        orientation: "<%= orientation.toLowerCase() %>"
    }, 
    events: {
        onChange: function(e) {
            <% if (events.onChange) { %>
                 $scope.factory['<%- events.onChange %>'](e);
            <% } %>
        }, onSlide: function(e) {
            <% if (events.onSlide) { %>
                 $scope.factory['<%- events.onSlide %>'](e);
            <% } %>
        }
    }
}