{
    widget: null,
    options: {
        resizable: <%- resizable %>,
        items: [
            <% items.forEach(function(item, index, arr) { %>
                {
                    <% if (typeof item.id !== "undefined") { %>
                    id: '<%- item.id %>', 
                    <% } %>
                    type: '<%- item.itemType ? item.itemType.toLowerCase() : "button" %>',
                    text: '<%- item.text %>',
                    togglable: <%- item.togglable %><% if (item.templateId !== "") { %>,
                    template: function(dataItem) {
                        return kendo.template(angular.element("#<%- item.templateId %>").html())(dataItem);
                    }<% } %><% if (item.templateId === "" && item.templateFunction !== "" && view.extension) { %>,
                    template: function(dataItem) { 
                        return $injector.get('<%- view.extension %>')['<%- item.templateFunction %>'](dataItem);
                    } <% } %>
                } <% if (index !== arr.length - 1) { %> , <% } %>
            <% }) %>
        ]
    },
    events: {
        onClick: function(e){
            <% if(view.extension && events.onClick) { %>
                $scope.factory['<%- events.onClick %>'](e);
            <% } %>
        }, onToggle: function(e){
            <% if(view.extension && events.onToggle) { %>
                $scope.factory['<%- events.onToggle %>'](e);
            <% } %>
        }
    }
}