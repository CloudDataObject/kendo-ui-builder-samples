<list-view
    data-id="<%- id %>"
    data-widget="<%- id %>.widget"
    <% if (model) { %>
    data-model="_$viewModels.<%- model %>",
    <% } else { %>
    data-model="_$viewModels.<%- id %>",
    <% } %>
    data-events="<%- id %>.events"
    data-options="<%- id %>.options"
    data-dom-ready="_$domReady">
</list-view>