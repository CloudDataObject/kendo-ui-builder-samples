{
    options: {
        defaultValue: "<%- defaultValue %>",
        placeholder: "<%= placeholder %>",
        debounce: <%= debounce %>
    }, 
    events: {
        onChange: function(e) {
            <% if (events.onChange) { %>
                 $scope.factory['<%- events.onChange %>'](e);
            <% } %>
        }
    },
    validation: {
        required: <%- validation.required %>
    }
}